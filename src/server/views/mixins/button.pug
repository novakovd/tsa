mixin button(id, text)
    button.text-white.bg-blue-700.font-medium.rounded-lg.text-sm.w-full.px-5.text-center(id=id data-secureId!=attributes.secureId type='button' class='hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 sm:w-auto py-2.5 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800')=text